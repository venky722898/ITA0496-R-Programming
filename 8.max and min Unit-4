scores <- c(85, 90, 78, 92, 88, 76, 95, 89, 84, 91, 87, 82, 90, 93, 88, 85, 77, 94, 80, 79)
min_score <- min(scores)
max_score <- max(scores)
range_score <- range(scores)
variance_score <- var(scores)
std_dev_score <- sd(scores)
cat("Minimum score:", min_score, "\n")
cat("Maximum score:", max_score, "\n")
cat("Range:", range_score, "\n")
cat("Variance:", variance_score, "\n")
cat("Standard Deviation:", std_dev_score, "\n")
